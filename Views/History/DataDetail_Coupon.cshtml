@model WebNails.Admin.Models.NailCoupon

<div class="row">
    <div class="col-md-12">
        <span id="TitlePartialView" style="display:none;">Lịch sử Coupon</span>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            @Html.Label("DateTimeCreate", "Thời gian")
            <div class="text-word-break">
                @Html.Raw(Model.DateTimeCreate)
            </div>
        </div>
        <div class="form-group">
            @Html.Label("Position", "Vị trí")
            <div class="text-word-break">
                @Html.Raw(Model.Position)
            </div>
        </div>
        <div class="form-group">
            @Html.Label("URL", "URL Coupon")
            <div class="text-word-break">
                @Html.Raw(Model.URL)
            </div>
        </div>
        <div class="form-group">
            @Html.Label("Status", "Bật/Tắt Image Coupon")
            <div class="text-word-break">
                @Html.Raw(Model.Status ? "Bật" : "Tắt")
            </div>
        </div>
    </div>
    <div class="col-md-6"></div>
</div>